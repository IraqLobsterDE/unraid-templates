  <Config Name="PICSUR_DB_PASSWORD" Target="PICSUR_DB_PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"/>
  <Config Name="PICSUR_DB_DATABASE" Target="PICSUR_DB_DATABASE" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">picsur</Config>
  <Config Name="PICSUR_ADMIN_PASSWORD" Target="PICSUR_ADMIN_PASSWORD" Default="" Mode="" Description="" Type="Variable" Display="always" Required="true" Mask="true"/>
